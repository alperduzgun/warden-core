{
  "name": "@warden/cli",
  "version": "0.1.0",
  "description": "Interactive CLI for Warden - AI-Powered DevSecOps Validation Platform",
  "main": "dist/index.js",
  "bin": {
    "warden-chat": "dist/index.js"
  },
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.tsx",
    "build": "tsc && chmod +x dist/index.js",
    "start": "node dist/index.js",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "warden",
    "cli",
    "devops",
    "security",
    "ink",
    "react"
  ],
  "author": "Warden Team",
  "license": "MIT",
  "dependencies": {
    "ink": "^6.2.3",
    "react": "^19.1.0",
    "ink-spinner": "^5.0.0",
    "ink-text-input": "^6.0.0",
    "ink-gradient": "^3.0.0",
    "ink-big-text": "^2.0.0",
    "zod": "^3.23.8",
    "chalk": "^5.4.1",
    "axios": "^1.7.9",
    "dotenv": "^16.4.7",
    "nanoid": "^5.0.9"
  },
  "devDependencies": {
    "@types/react": "^19.0.6",
    "@types/node": "^22.10.5",
    "typescript": "^5.3.3",
    "tsx": "^4.19.2",
    "@typescript-eslint/eslint-plugin": "^8.19.1",
    "@typescript-eslint/parser": "^8.19.1",
    "eslint": "^9.17.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.1.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
